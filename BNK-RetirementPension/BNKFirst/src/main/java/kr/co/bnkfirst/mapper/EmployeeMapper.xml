<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.bnkfirst.mapper.EmployeeMapper">

    <select id="findById" parameterType="long" resultType="kr.co.bnkfirst.domain.Employee">
        SELECT *
        FROM EMPLOYEE
        WHERE EMPID = #{empId}
    </select>

    <select id="findAll" resultType="kr.co.bnkfirst.domain.Employee">
        SELECT *
        FROM EMPLOYEE
    </select>

</mapper>
